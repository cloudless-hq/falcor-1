{
  "name": "@graphistry/falcor-react-schema",
  "version": "2.7.0",
  "description": "",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "clean": "rimraf lib && mkdirp lib",
    "ehprepublish": "npm run build",
    "build": "npm run clean && babel src -d lib --source-maps",
    "watch": "npm run clean && babel src -d lib --source-maps --watch",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i --env jest-environment-node-debug"
  },
  "keywords": [
    "falcor",
    "falcor-router"
  ],
  "files": [
    "src",
    "lib"
  ],
  "author": "Graphistry",
  "license": "ISC",
  "dependencies": {
    "@graphistry/falcor": "^2.7.0",
    "@graphistry/falcor-json-graph": "^2.6.16",
    "@graphistry/falcor-path-utils": "^2.6.16",
    "@graphistry/falcor-query-syntax": "^2.7.0",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-preset-react": "~6.22.0",
    "invariant": "~2.2.2",
    "pegjs-util": "~1.4.1",
    "react": "~15.4.1",
    "recompose": "~0.22.0",
    "rxjs": "~5.1.0"
  },
  "devDependencies": {
    "@graphistry/falcor-model-rxjs": "^2.7.0",
    "@graphistry/falcor-router": "^2.7.0",
    "babel-cli": "~6.22.2",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-stage-0": "~6.22.0",
    "cross-env": "~3.1.3",
    "enzyme": "^2.7.1",
    "jest": "^18.1.0",
    "jest-environment-node-debug": "^0.0.2",
    "jest-enzyme": "^2.1.1",
    "mkdirp": "~0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "~2.5.4"
  },
  "jest": {
    "verbose": true,
    "testPathDirs": [
      "src/"
    ],
    "testRegex": "(/__tests__/.*-(test|spec)s?)\\.js$",
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js"
  }
}
