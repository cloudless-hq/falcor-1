    "@graphistry/falcor-query-syntax": "^2.1.7",
    "babel-core": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-es2016": "6.11.3",
    "babel-preset-stage-0": "6.5.0",
    "benchmark": "2.1.1",
