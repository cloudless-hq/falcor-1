    "del": "1.2.1",
    "es6-promise": "3.3.0",
    "express": "4.13.3",
    "falcor": "netflix/falcor",
    "falcor-express": "0.1.2",
    "falcor-http-datasource": "0.1.3",
    "falcor-router": "0.4.0",
    "falcor-router-demo": "1.0.3",
